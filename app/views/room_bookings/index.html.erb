<h1>Listing room_bookings</h1>

<table>
  <tr>
    <th>Type</th>
    <th>Cancelled</th>
    <th>Description</th>
    <th>Booked for</th>
    <th>Start</th>
    <th>End</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @room_bookings.each do |room_booking| %>
  <tr>
    <td><%= room_booking.type %></td>
    <td><%= room_booking.cancelled %></td>
    <td><%= room_booking.description %></td>
    <td><%= room_booking.booked_for %></td>
    <td><%= room_booking.start %></td>
    <td><%= room_booking.end %></td>
    <td><%= link_to 'Show', room_booking %></td>
    <td><%= link_to 'Edit', edit_room_booking_path(room_booking) %></td>
    <td><%= link_to 'Destroy', room_booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Room booking', new_room_booking_path %>
